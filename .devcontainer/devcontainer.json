{
	"name": "news-manager",
	"dockerComposeFile": [
		"../.docker/docker-compose.local.yaml"
	],
	"service": "news-manager-api",
	"workspaceFolder": "/home/docker/workspace",
	"features": {},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"charliermarsh.ruff",
				"bierner.markdown-mermaid"
			],
			"settings": {
				"python.defaultInterpreterPath": "/home/docker/workspace/.venv/bin/python"
			}
		}
	}
}